<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="redmine.webguild.redmineandroid"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="9" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="redmine.webguild.redmineandroid.MainActivity"
            android:label="@string/app_name"
             >
            <intent-filter>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="enter">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="home"></activity>
        <activity android:name="mypage"></activity>
        <activity android:name="project_activity"></activity>
        <activity android:name="project_add_news"></activity>
        <activity android:name="project_add_task"></activity>
        <activity android:name="project_add_worker"></activity>
        <activity android:name="project_main"></activity>
        <activity android:name="project_news"></activity>
        <activity android:name="project_settings_worker"></activity>
        <activity android:name="project_settings"></activity>
        <activity android:name="project_task_edit"></activity>
        <activity android:name="project_task"></activity>
        <activity android:name="project_tasks_list"></activity>
        <activity android:name="projects"></activity>
        <activity android:name="worker_page"></activity>
    </application>
<uses-permission android:name="android.permission.INTERNET" /> 
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
</manifest>